{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/__pycache__": true,
        ".vim": true,
        "README.md": true,
        ".env.example": true,
        "venv": true,
        "docs": true,
    },
    "hide-files.files": [],
    "editor.autoIndent": "full",
    "editor.formatOnSave": true,
    "python.testing.unittestArgs": [
        "-v",
        "-s",
        "./venv",
        "-p",
        "*_test.py"
    ],
    "python.testing.pytestEnabled": false,
    "python.testing.unittestEnabled": true,
    "terminal.integrated.ignoreProcessNames": [
        "starship",
        "oh-my-posh",
        "bash"
    ],
}
